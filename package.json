{
  "name": "semantic-release-playground",
  "version": "6.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AllanGuigou/semantic-release-playground.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AllanGuigou/semantic-release-playground/issues"
  },
  "homepage": "https://github.com/AllanGuigou/semantic-release-playground#readme",
  "release": {
    "branches": [
      "main"
    ],
    "npmPublish": false,
    "private": true,
    "plugins": [
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "angular",
          "parserOpts": {
            "noteKeywords": [
              "BREAKING CHANGE",
              "BREAKING CHANGES",
              "BREAKING"
            ]
          },
          "writerOpts": {
            "commitsSort": [
              "subject",
              "scope"
            ]
          }
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "index.js"
          ],
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/release-notes-generator": "^9.0.3"
  }
}
